C
#include <stdio.h>
void multiplyPoly(int A[], int B[], int res[], int n, int m) {
    for (int i = 0; i < n + m - 1; i++)
        res[i] = 0;
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            res[i + j] += A[i] * B[j];
}

Python 
def multiply_polynomials(A, B):
    result = [0] * (len(A) + len(B) - 1)
    for i in range(len(A)):
        for j in range(len(B)):
            result[i + j] += A[i] * B[j]
    return result
