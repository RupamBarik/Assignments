#include <stdio.h>
int main() {
    int n, arr[100], largest;
    printf("Enter size of array: ");
    scanf("%d", &n);
    printf("Enter %d elements: ", n);
    for(int i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    largest = arr[0];
    for(int i = 1; i < n; i++)
        if(arr[i] > largest)
            largest = arr[i];
    printf("Largest element: %d\n", largest);
    return 0;
}
