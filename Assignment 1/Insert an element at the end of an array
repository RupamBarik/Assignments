#include <stdio.h>
int main() {
    int arr[100], n, element;
    printf("Enter size of array: ");
    scanf("%d", &n);
    printf("Enter %d elements: ", n);
    for(int i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    printf("Enter element to insert at end: ");
    scanf("%d", &element);
    arr[n] = element; // Insert at last position
    n++;
    printf("Array after insertion: ");
    for(int i = 0; i < n; i++)
        printf("%d ", arr[i]);
    return 0;
}
