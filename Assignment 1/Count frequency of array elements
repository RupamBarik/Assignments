C

#include <stdio.h>
int main() {
    int n, arr[100], freq[100] = {0};
    scanf("%d", &n);
    for(int i=0; i<n; i++) scanf("%d", &arr[i]);
    for(int i=0; i<n; i++) {
        if(freq[i]) continue;
        int count = 1;
        for(int j=i+1; j<n; j++)
            if(arr[i] == arr[j]) { count++; freq[j]=1; }
        printf("%d occurs %d times\n", arr[i], count);
    }
    return 0;
}


Python

arr = list(map(int, input().split()))
freq = {}
for num in arr:
    freq[num] = freq.get(num, 0) + 1
for k, v in freq.items():
    print(f"{k} occurs {v} times")
